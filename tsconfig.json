  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "sourceMap": true,
    "outDir": "./dist",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
}